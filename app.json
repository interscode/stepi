{
  "expo": {
    "name": "stepi",
    "slug": "stepi",
    "version": "1.0.0",
    "scheme": "com.interscode.stepi",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#330072"
      },
      "package": "com.interscode.stepi"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#330072",
          "image": "./assets/splash-icon.png",
          "imageWidth": 200
        }
      ],
      "expo-font"
    ]
  }
}
